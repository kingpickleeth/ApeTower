{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      },
      {
        "src": "api/**/*.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  