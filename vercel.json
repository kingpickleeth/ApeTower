{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build"
          }
          ,
      {
        "src": "api/**/*.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
          "src": "/api/tower/(\\d+)",
          "dest": "/api/tower/[id].ts"
        },
        {
          "src": "/(.*)",
          "dest": "/index.html"
        }
      ]
    }